<configuration>
	<configSections>
		<section name="webHostConfiguration" type="DotJEM.Web.Host.Configuration.Elements.WebHostConfiguration, DotJEM.Web.Host, Version=1.0.0.0, Culture=neutral" />
	</configSections>
	<webHostConfiguration>
		<storageConfiguration connectionString="Data Source=.\DEV;Initial Catalog=ssn3db;Integrated Security=True">
			<area name="*" />
		</storageConfiguration>
		<indexConfiguration>
			<storage type="File" path="/APP_DATA/index" />
			<!--
        batch-size: The number of change elements processed at a time.
        
        Valid on nodes:
          - watch
          - add
        
        Adding a batch-size configuration on the watch node sets the batch-size for all child nodes.
        The batch-size can then be overwritten by a single node by adding it to that node as well, e.g.
        
        <watch interval="10" rambuffer="1024MB" batch-size="5000">
          <add area="content" />
          <add area="log" batch-size="10000"/>
          <add area="data" batch-size="500"/>
          <add area="files" />
        </watch>
        
        Where the watcher will load 5000 elements at a time for the content and files storage area as configured for all.
        However the watcher will load 10000 from the log storage area and only 500 from the data storage area as these has been
        individually overwritten.
         -->
			<debug enabled="true">
				<index-writer-info-stream path="/APP_DATA/indexlogs/writer.log" max-size="16MB" max-files="100" zip="true" />
			</debug>
			<watch interval="10" rambuffer="1024MB" batch-size="10000">
				<add area="content" />
				<add area="diagnostic" />
				<add area="emsaqueue" />
				<add area="settings" />
			</watch>
			<snapshots max-snapshots="5" path="/APP_DATA/snapshots" cron-time="0 * * * *" />
		</indexConfiguration>
		<diagnostics>
			<performance path="/APP_DATA/performance/perf.log" max-size="16MB" max-files="100" zip="true" />
		</diagnostics>
		<cleanup interval="10s">
			<add query="$contentType:cleanme AND $created:[* TO -30sec]"></add>
		</cleanup>
	</webHostConfiguration>
	<appSettings>
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
	</appSettings>
	<!--
